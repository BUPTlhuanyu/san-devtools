{
    "name": "san-devtools",
    "version": "1.0.0",
    "description": "san-devtools",
    "private": true,
    "scripts": {
        "start:ext": "yarn workspace extensions start",
        "start": "yarn dev:todo",
        "dev:standalone": "yarn workspace san-devtools start",
        "build:standalone": "yarn workspace san-devtools build",
        "build:extensions": "yarn workspace extensions build",
        "dev:todo": "yarn workspace shell-dev start"
    },
    "workspaces": [
        "packages/*"
    ],
    "files": [
        "packages"
    ],
    "dependencies": {
        "san": "^3.9.2",
        "san-router": "^1.2.2"
    },
    "devDependencies": {
        "san-hot-loader": "^0.1.2",
        "@typescript-eslint/parser":"2.34.0",
        "@typescript-eslint/eslint-plugin":"2.34.0",
        "@babel/core": "^7.9.6",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-export-default-from": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.10.1",
        "@babel/plugin-transform-modules-commonjs": "^7.10.1",
        "babel-plugin-import": "^1.13.0",
        "@babel/preset-env": "^7.9.6",
        "@commitlint/cli": "^8.3.4",
        "@commitlint/core": "^8.3.4",
        "@commitlint/travis-cli": "^8.3.5",
        "@ecomfe/eslint-config": "^3.2.0",
        "@ksky521/cz-emoji": "^1.2.1",
        "babel-eslint": "^10.1.0",
        "commitizen": "^4.0.3",
        "commitlint-config-gitmoji": "^1.0.1",
        "concurrently": "^5.2.0",
        "eslint": "^6.8.0",
        "eslint-plugin-babel": "^5.3.0",
        "html-loader": "^1.1.0",
        "jquery": "^3.5.1",
        "webpack": "^4.43.0",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^5.1.2"
    },
    "lint-staged": {
        "!(example|dist|output|__mocks__)/**/*.(ts|js)": [
            "eslint"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "commitlint": {
        "extends": [
            "gitmoji"
        ],
        "rules": {
            "header-max-length": [
                2,
                "always",
                120
            ],
            "subject-case": [
                0,
                "always",
                "sentence-case"
            ]
        }
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/@ksky521/cz-emoji"
        },
        "@ksky521/cz-emoji": {
            "types": [
                {
                    "emoji": "üåü",
                    "code": ":sparkles:",
                    "description": "Introducing new features.",
                    "name": "feat"
                },
                {
                    "emoji": "üêõ",
                    "code": ":bug:",
                    "description": "Fixing a bug.",
                    "name": "fix"
                },
                {
                    "emoji": "üöë",
                    "code": ":ambulance:",
                    "description": "Critical hotfix.",
                    "name": "quickfix"
                },
                {
                    "emoji": "‚úÖ",
                    "code": ":white_check_mark:",
                    "description": "Adding tests.",
                    "name": "test"
                },
                {
                    "emoji": "üí¨",
                    "code": ":speech_balloon:",
                    "description": "Updating text and literals.",
                    "name": "chore"
                },
                {
                    "emoji": "üì¶",
                    "code": ":package:",
                    "description": "Releasing / Version tags.",
                    "name": "release"
                },
                {
                    "emoji": "üìù",
                    "code": ":pencil:",
                    "description": "Writing docs.",
                    "name": "docs"
                },
                {
                    "emoji": "üö®",
                    "code": ":rotating_light:",
                    "description": "Removing linter warnings.",
                    "name": "lint"
                },
                {
                    "emoji": "‚è™",
                    "code": ":rewind:",
                    "description": "Reverting changes.",
                    "name": "revert"
                },
                {
                    "emoji": "üîÄ",
                    "code": ":twisted_rightwards_arrows:",
                    "description": "Merging branches.",
                    "name": "merge"
                },
                {
                    "emoji": "üëå",
                    "code": ":ok_hand:",
                    "description": "Updating code due to code review changes.",
                    "name": "review"
                },
                {
                    "emoji": "üé®",
                    "code": ":art:",
                    "description": "Improving structure / format of the code.",
                    "name": "style"
                },
                {
                    "emoji": "üíÑ",
                    "code": ":lipstick:",
                    "description": "Updating the UI and style files.",
                    "name": "ui"
                },
                {
                    "emoji": "üëΩ",
                    "code": ":alien:",
                    "description": "Updating code due to external API changes.",
                    "name": "compat"
                },
                {
                    "emoji": "üí•",
                    "code": ":boom:",
                    "description": " Refactoring code.",
                    "name": "refactor"
                },
                {
                    "emoji": "üéâ",
                    "code": ":tada:",
                    "description": "Initial commit.",
                    "name": "init"
                }
            ],
            "skipQuestions": [
                "scope",
                "body",
                "issues"
            ]
        }
    },
    "repository": "ssh://git@icode.baidu.com:8235/baidu/hulk/san-devtools",
    "author": "wangyongqing01 <wangyongqing01@baidu.com>",
    "license": "MIT"
}
