        "sand": "bin/san-devtools.js"
    },
    "engines": {
        "node": ">=8.16"
    },
    "keywords": [
        "san",
        "san-devtools",
        "devtool",
        "san-devtool",
        "chrome",
        "debug",
        "extension"
    ],
    "files": [
        "bin",
        "dist",
        "public",
        "server",
        "package.json",
        "README.md"
    ],
